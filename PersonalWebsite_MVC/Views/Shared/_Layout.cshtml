
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

    <!-- Meta Tag -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- SEO -->
    <meta name="description" content="150 words">
    <meta name="author" content="uipasta">
    <meta name="url" content="http://www.yourdomainname.com">
    <meta name="copyright" content="company name">
    <meta name="robots" content="index,follow">


    <title>@ViewBag.Title</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="/Content/images/favicon/favicon.ico">
    <link rel="apple-touch-icon" sizes="144x144" type="image/x-icon" href="/Content/images/favicon/apple-touch-icon.png">

    <!-- All CSS Plugins -->
    <link rel="stylesheet" type="text/css" href="/Content/css/plugin.css">

    <!-- Main CSS Stylesheet -->
    <link rel="stylesheet" type="text/css" href="/Content/css/style.css">

    <!-- Google Web Fonts  -->
    <link rel="stylesheet" href="/Content/https://fonts.googleapis.com/css?family=Poppins:400,300,500,600,700">


    <!-- HTML5 shiv and Respond.js support IE8 or Older for HTML5 elements and media queries -->
    <!--[if lt IE 9]>
       <script src="/Content/https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
       <script src="/Content/https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    @RenderSection("head", false)
</head>
<body>



    <!-- Preloader Start -->
   
    <!-- Preloader End -->
    <!-- Menu Section Start -->
    <header id="home">

        <div class="header-top-area">
            <div class="container">
                <div class="row">

                    <div class="col-sm-3">
                        <div class="logo">
                            <a href="/Home/Index">SALİH</a>
                        </div>
                    </div>

                    <div class="col-sm-9">
                        <div class="navigation-menu">
                            <div class="navbar">
                                <div class="navbar-header">
                                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                        <span class="sr-only">Toggle navigation</span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>
                                </div>
                                <div class="navbar-collapse collapse">
                                    <ul class="nav navbar-nav navbar-right">
                                        <li class="active">
                                            <a class="smoth-scroll" href="/Home/Index">ANA SAYFA <div class="ripple-wrapper"></div></a>
                                        </li>
                                        <li>
                                            <a class="smoth-scroll" href="/About/Index">HAKKIMDA</a>
                                        </li>
                                        <li>
                                            <a class="smoth-scroll" href="/Project/Index">PROJELERİM</a>
                                        </li>
                                        <li>
                                            <a class="smoth-scroll" href="/MyTeam/Index">EKİBİM</a>
                                        </li>
                                        <li>
                                            <a class="smoth-scroll" href="/Service/Index">HİZMETLERİM</a>
                                        </li>
                                        <li>
                                            <a class="smoth-scroll" href="/Blog/Index">BLOG</a>
                                        </li>
                                        
                                        <li>
                                            <a class="smoth-scroll" href="/SignUp/Index">KAYIT OL</a>
                                        </li>
                                       
                                        <li>
                                            <a class="smoth-scroll" href="/Contact/Index">İLETİŞİM</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    
    
       @RenderBody()
   

    

    <footer class="footer-section">
        <div class="container">
            <div class="row">

                <div class="col-md-12">
                    <ul class="social-icon margin-bottom-30">
                        <li><a href="https://www.facebook.com/salihcan.yilmaz.9" target="_blank" class="facebook"><i class="icon-social-facebook"></i></a></li>
                        <li><a href="/Content/#" target="_blank" class="twitter"><i class="icon-social-twitter"></i></a></li>
                        <li><a href="/Content/#" target="_blank" class="google-plus"><i class="icon-social-google"></i></a></li>
                        <li><a href="/Content/#" target="_blank" class="instagram"><i class="icon-social-instagram"></i></a></li>
                        
                    </ul>
                </div>

                <div class="col-md-12 uipasta-credit">
                    <p>Design By <a href="https://www.facebook.com/salihcan.yilmaz.9" target="_blank" title="UiPasta">Salih Can YILMAZ</a></p>
                </div>

            </div>
        </div>
    </footer>
    <!-- Footer End -->
    <!-- Back to Top Start -->
    <a href="/Content/#" class="scroll-to-top"><i class="icon-arrow-up-circle"></i></a>
    <!-- Back to Top End -->
    <!-- All Javascript Plugins  -->
    <script type="text/javascript" src="/Content/js/jquery.min.js"></script>
    <script type="text/javascript" src="/Content/js/plugin.js"></script>

    <!-- Main Javascript File  -->
    <script type="text/javascript" src="/Content/js/scripts.js"></script>

    @RenderSection("scripts",false)

</body>
</html>